<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:drawable="@drawable/tui_progressbar_bg"/>
	<item android:id="@android:id/progress">
	    <clip>
	         <nine-patch android:src="@drawable/tui_progressbar_normal"></nine-patch>
	    </clip>
	</item>
	<item android:id="@android:id/secondaryProgress">
    	<clip>
        	<nine-patch android:src="@drawable/tui_progressbar_loaded"></nine-patch>
    	</clip>
	</item>
</layer-list>
